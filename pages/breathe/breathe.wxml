<!--pages/breathe/breathe.wxml-->
<view class="container">
  <!-- 自定义动画效果 -->
  <view class="custom-animation" animation="{{ animationData }}"></view>
  <!-- 呼吸提示文本 -->
  <view class="breath-text">{{ breathText }}</view>
  <!-- 启动按钮 -->
</view>
<view class="button-container"><button bindtap="startAnimation">开始练习</button></view>