.container {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.custom-animation {
  width: 100rpx;
  height: 100rpx;
  background-color: rgb(26, 79, 236);
  border-radius: 50%; /* 将动画元素变为圆形 */
}

.button-container {
  position: absolute;
  bottom: 600rpx;
  /* 距离内容底部的距离 */
  left: 50%;
  transform: translateX(-50%)
}
